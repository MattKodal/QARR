{% block pageTitle %}
    <div class="page-title-wrapper">
        <a href="{{ url('qarr') }}" class="qarr-logo">
            {% include 'qarr/_includes/icons/qarr-logo' ignore missing %}
        </a>

        <div class="page-title">
            <h1>{{ title }}</h1>
            {% include 'qarr/_includes/header/crumbs' ignore missing %}
        </div>

    </div>

    <div class="utility-nav">
        <div class="utility-nav-item">
            <div class="nav-icon">
                <span class="fa-layers fa-fw">
                    <i class="fal fa-bell"></i>
                    <span class="fa-layers-counter" style="background:Tomato"></span>
                  </span>

            </div>
        </div>
    </div>
{% endblock %}